{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [],
			"outputs": ["dist/**/*", "docs/**/*"]
		},
		"bundle": {
			"dependsOn": [],
			"outputs": ["storybook_static/**/*"]
		},
		"test": {
			"dependsOn": [],
			"outputs": ["coverage/**/*"]
		},
		"check": {
			"dependsOn": [],
			"outputs": []
		},
		"check-deps": {
			"dependsOn": [],
			"outputs": []
		},
		"fix": {
			"dependsOn": [],
			"outputs": []
		},
		"ci": {
			"dependsOn": ["build", "check", "test", "check-deps", "bundle"],
			"outputs": ["dist/**/*", "docs/**/*", "storybook_static/**/*", "coverage/**/*"]
		},
		"clean": {
			"cache": false
		},
		"release": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"stories": {
			"cache": false
		}
	}
}
