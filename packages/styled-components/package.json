{
	"name": "@essex/styled-components",
	"description": "ESM export support for styled-components",
	"version": "0.0.4",
	"type": "module",
	"main": "src/index.ts",
	"publishConfig": {
		"main": "dist/index.js",
		"types": "dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"clean": "essex clean",
