	},
	"devDependencies": {
		"@essex/scripts": "^22.0.7",
		"@essex/tsconfig-base": "^1.0.2",
		"@types/styled-components": "^5.1.26",
		"styled-components": "^5.3.6",
		"typescript": "^4.9.4"
	},
	"peerDependencies": {
		"styled-components": ">= 5"
	}
}
