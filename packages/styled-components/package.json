{
  "name": "@essex/styled-components",
  "description": "ESM export support for styled-components",
  "version": "0.0.6",
  "type": "module",
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "essex clean",
    "build": "essex build --docs",
    "fix": "essex fix --formatter none",
    "check": "essex check --formatter none",
    "release": "yarn npm publish --tolerate-republish --access public"
  },
  "devDependencies": {
    "@essex/scripts": "^23.0.2",
    "@essex/tsconfig-base": "^1.0.2",
    "@types/styled-components": "^5.1.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "styled-components": ">= 5"
  },
  "peerDependenciesMeta": {
    "styled-components": {
      "optional": false
    }
  }
}
