{
	"name": "@essex-js-toolkit/dev",
	"version": "1.2.2",
	"description": "A set of packages for developers",
	"repository": "git@ssh.dev.azure.com:v3/msrp-essex/Alpha/alpha-developer",
	"author": "David Tittsworth <datittsw@microsoft.com>",
	"contributors": [
		"David Tittsworth <datittsw@microsoft.com>",
		"Nathan Evans <naevans@microsoft.com>",
		"Chris Trevino <chtrevin@microsoft.com>",
		"Amber Hoak <amhoak@microsoft.com>",
		"Eric Kimbrel <lekimbrel@gmail.com>",
		"Derek Worthen <deworthe@microsoft.com>",
		"Dayenne Souza <ddesouza@microsoft.com>"
	],
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">= 12.0"
	},
	"scripts": {
		"preinstall": "npx only-allow yarn",
		"postinstall": "husky install",
		"clean": "yarn workspaces foreach -pv run clean",
		"build": "yarn workspaces foreach -pvt run build",
		"test": "yarn workspaces foreach -pv run test",
		"start:": "yarn workspaces foreach -piv run start",
		"release": "yarn workspaces foreach --exclude '@essex-js-toolkit/dev' -pivt npm publish --tolerate-republish --access public",
		"jest_coverage": "jest --coverage",
		"lint": "essex lint --strict",
		"lint_fix": "yarn lint --fix",
		"git_is_clean": "essex git-is-clean",
		"format": "essex prettify",
		"ci": "run-s build lint_fix test jest_coverage git_is_clean"
	},
	"devDependencies": {
		"@essex/eslint-config": "^20.0.0",
		"@essex/eslint-plugin": "^20.0.0",
		"@essex/jest-config": "^20.1.1",
		"@essex/prettier-config": "^18.0.2",
		"@essex/scripts": "^20.3.0",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.18",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"debug": "^4.3.3",
		"eslint": "^8.9.0",
		"eslint-import-resolver-node": "^0.3.6",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "^12.3.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"regenerator-runtime": "^0.13.9",
		"typescript": "^4.5.5"
	},
	"workspaces": [
		"packages/*"
	],
	"resolutions": {
		"handlebars": "4.4.3",
		"@types/react": "17.0.9",
		"@types/react-dom": "17.0.6"
	},
	"packageManager": "yarn@3.1.1"
}
